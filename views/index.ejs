            <form class="pesquisa" action="/resultados" method="POST">
              
               <input class="pesquisaI" type="text" name="resultados" placeholder="Discord Bot List" required>
               <button class="pesquisaB" type="submit"><i class="fa fa-search"></i></button>
              
             </form>

            -->
          
        </div>  
        
        <div class="info">
        <div>
                                                <center><li><button type="submit" class="dropbtn"><%- user.tag %></button></li></center>
              <center><img class="userAvatar" src="<%- userinfo.avatarURL({ dynamic: true, size: 1024 }) %>"></center></br></br></br>
              
              <h1 class="perfil">Lista de Bots</h1></br></br></br></br>

<div class="cards">
  
  <% Bots.forEach(function(bot) { %>
  
    <div class="card">

         <img src="<%- bot.avatar %>" alt="Avatar" style="width:100%"></br>
  
            <div class="container">
           
                <h4><b class="title" style="color:white; font-size:20"><%- bot.name %></b></h4></br> 
                <a href=<%- `/bots/${bot._id}` %>><button class="botsInvite">Ver Bot</button></a></br>

                <!-- <a href=<%- `/bots/${bot._id}` %>><button class="botsInvite">Invite</button></a> -->
                
            </div>
  
  </div>

<% }); %>
          
  

</div>
</div>
</div>
